{
  "name": "raf-stub",
  "version": "1.0.1",
  "description": "Accurate and predictable testing of requestAnimationFrame and cancelAnimationFrame",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "performance-now": "0.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "3.8.1",
    "isparta": "4.0.0",
    "mocha": "3.1.2",
    "sinon": "1.17.6"
  },
  "scripts": {
    "test": "npm run lint && ./node_modules/.bin/mocha test --compilers js:babel-core/register",
    "lint": "./node_modules/.bin/eslint src test",
    "lint-fix": "npm run lint - --fix",
    "compile": "./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run compile",
    "coverage": "node_modules/.bin/isparta cover --include-all-sources node_modules/mocha/bin/_mocha -- test --compilers js:babel-core/register",
    "codecov": "npm run coverage && ./node_modules/.bin/codecov"
  },
  "keywords": [
    "raf",
    "requestAnimationFrame",
    "cancelAnimationFrame",
    "test",
    "testing",
    "mocha",
    "react-motion"
  ],
  "author": "Alex Reardon <alexreardon@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexreardon/raf-stub.git"
  }
}
